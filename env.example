# Team Ops Assistant - Environment Variables
# Copy this file to .env and fill in your values

# MongoDB Atlas connection string
MONGODB_URI=mongodb+srv://user:password@cluster.mongodb.net/team_ops?retryWrites=true&w=majority

# Telegram Bot Token (from @BotFather)
TELEGRAM_BOT_TOKEN=your-telegram-bot-token

# Discord Bot Token (from Discord Developer Portal)
DISCORD_BOT_TOKEN=your-discord-bot-token

# WhatsApp Cloud API credentials
WHATSAPP_ACCESS_TOKEN=your-whatsapp-access-token
WHATSAPP_PHONE_NUMBER_ID=your-phone-number-id
WHATSAPP_VERIFY_TOKEN=your-webhook-verify-token

# Together AI API key (for Qwen3-Next-80B-A3B-Instruct)
TOGETHER_API_KEY=your-together-api-key

# Application settings
APP_SECRET_KEY=change-this-to-a-random-secret-key
APP_HOST=0.0.0.0
APP_PORT=8000
APP_DEBUG=false

# Verify token signing secret
VERIFY_TOKEN_SECRET=change-this-to-a-random-secret
